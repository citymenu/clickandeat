<decorators defaultdir="/WEB-INF/decorators">
    <excludes>
        <pattern>*.js</pattern>
    </excludes>
    <decorator name="admin" page="admin.jsp">
        <pattern>/admin/*</pattern>
    </decorator>
    <decorator name="blank" page="blank.jsp">
        <pattern>/cardProcessing*</pattern>
    </decorator>
    <decorator name="static" page="static.jsp">
        <pattern>/legal*</pattern>
    </decorator>
    <decorator name="default" page="site.jsp">
		<pattern>/*</pattern>
  	</decorator>

</decorators>